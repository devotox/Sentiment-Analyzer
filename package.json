{
	"name": "sentiment-analyzer",
	"version": "1.1.5",
	"description": "Sentiment Analyzer",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/devotox/sentiment-analyzer"
	},
	"scripts": {
		"flow": "flow",
		"demo": "babel-node demo/demo",
		"demo:test": "babel-node demo/test",
		"pretest": "npm run jsinspect && npm run lint",
		"jsinspect": "jsinspect -t 100 ./src",
		"test": "mocha tests/",
		"lint": "eslint ./",
		"postrelease": "npm publish",
		"release": "npm version patch",
		"release-patch": "npm version patch",
		"release-minor": "npm version minor",
		"release-major": "npm version major",
		"postversion": "git push && git push --tags",
		"build": "rm -rf ./dist/* && ./node_modules/.bin/babel src -d dist",
		"dev": "rm -rf ./dist/* && ./node_modules/.bin/babel -w src -d dist",
		"preversion": "sidekick run && npm run build && npm test"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@google-cloud/language": "1.1.0",
		"axios": "0.17.1",
		"bluebird": "3.5.1",
		"cheerio": "1.0.0-rc.2",
		"fluture": "8.0.0",
		"google-finance": "devotox/node-google-finance",
		"google-stocks": "2.0.1",
		"he": "1.1.1",
		"indico.io": "0.10.5",
		"lodash": "4.17.4",
		"moment": "2.20.1",
		"ramda": "0.25.0",
		"request": "2.83.0",
		"retext": "5.0.0",
		"retext-sentiment": "4.1.0",
		"textversionjs": "1.0.2",
		"yahoo-finance": "0.3.5"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-eslint": "8.2.1",
		"babel-plugin-transform-flow-strip-types": "6.22.0",
		"babel-preset-flow": "6.23.0",
		"eslint-plugin-flowtype": "2.41.0",
		"eslint": "4.15.0",
		"flow-bin": "0.63.1",
		"jsinspect": "0.12.7",
		"mocha": "4.1.0",
		"sidekick": "1.12.2"
	}
}
