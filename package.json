{
	"name": "sentiment-analyzer",
	"version": "2.0.7",
	"description": "Sentiment Analyzer",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/devotox/sentiment-analyzer"
	},
	"scripts": {
		"flow": "flow",
		"demo": "babel-node demo/demo",
		"demo:test": "babel-node demo/test",
		"pretest": "npm run jsinspect && npm run lint",
		"jsinspect": "jsinspect -t 100 ./src",
		"test": "mocha --require babel-core/register tests/*",
		"lint": "eslint ./",
		"postrelease": "npm publish",
		"release": "npm version patch",
		"release-patch": "npm version patch",
		"release-minor": "npm version minor",
		"release-major": "npm version major",
		"postversion": "git push && git push --tags",
		"preversion": "sidekick run && npm run build && npm test",
		"build": "rm -rf ./dist/* && ./node_modules/.bin/babel src -d dist",
		"dev": "rm -rf ./dist/* && ./node_modules/.bin/babel -w src -d dist",
		"update:packages": "npx ncu -u --removeRange && yarn install --force --check-files --update-checksums --ignore-engines && yarn upgrade --force --ignore-engines"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@google-cloud/language": "3.7.2",
		"axios": "0.19.2",
		"bluebird": "3.7.2",
		"cheerio": "1.0.0-rc.3",
		"fluture": "12.2.0",
		"he": "1.2.0",
		"indico.io": "0.10.6",
		"lodash": "4.17.21",
		"moment": "2.24.0",
		"ramda": "0.27.0",
		"request": "2.88.2",
		"retext": "7.0.1",
		"retext-sentiment": "4.1.2",
		"textversionjs": "1.1.3",
		"yahoo-finance": "0.3.6"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-eslint": "10.0.3",
		"babel-plugin-transform-flow-strip-types": "6.22.0",
		"babel-preset-flow": "6.23.0",
		"chai": "4.2.0",
		"eslint": "6.8.0",
		"eslint-plugin-flowtype": "4.6.0",
		"flow-bin": "0.118.0",
		"jsinspect": "0.12.7",
		"mocha": "7.0.1",
		"sidekick": "1.12.2"
	}
}
